@inproceedings{Chen2004Towards-A-Modal,
	Abstract = {The pi calculus is one of the most important mo- 
bile process calculi and has been wel l studied in litera- 
tures. Temporal logic is thought as a good compromise 
between description convenience and abstraction and 
can support useful computational applications, such as 
model-checking. In this paper, we use symbolic tran- 
sition graph inherited from pi calculus to model con- 
current systems. A wide class of processes, that is, 
the finite-control processes can be represented as finite 
symbolic transition graph. A new version modal logic 
for pi calculus, an extension of the modal mu-calculus 
with boolean expressions over names, and primitives for 
name input and output are introduced as an appropriate 
temporal logic for the pi calculus. Since we make a dis- 
tinction between proposition and predicate, the possible 
interactions between recursion and first-order quantifi- 
cation can be solved. A concise semantics interpreta- 
tion for our modal logic is given. Based on the above 
work, we provide a model checking algorithm for the 
logic. This algorithm fol lows the wel l-known Winskel's 
tag set method to deal with fixpoint operator. As for 
the problem of name instantiating, our algorithm fol- 
lows the 'on-the-fly' style, and systematical ly employs 
schematic names. The correctness of the algorithm is 
shown. 
},
	Author = {Taolue Chen and Tingting Han and Jian Lu},
	Booktitle = {Proc. of of the 28th Annual International Computer Software and Applications Conference (COMPSAC'04)},
	Date-Added = {2007-09-13 23:13:31 +0200},
	Date-Modified = {2007-09-13 23:13:31 +0200},
	Keywords = {pi calculus, Symbolic Transition Graph, Modal Logic, Model Checking Algorithm.},
	Local-Url = {file://localhost/Users/halopez/Documents/Papers/Chen/Towards%20A%20Modal%20Logic%20For%20Pi%20Calculus.pdf},
	Title = {Towards a Modal Logic for Pi Calculus},
	Year = {2004},
	Bdsk-File-1 = {}}

@book{Winskel1997Categories-in-C,
	Abstract = {These notes survey a range of models for parallel computation, includ- 
ing interleaving models like transition systems, synchronisation trees and 
languages (often called Hoare traces in this context), and models like asyn- 
chronous transition systems, event structures, pomsets and Mazurkiewicz 
traces where concurrency is represented more explicitly by a form of causal 
independence. The presentation is unified by casting the models in a category- 
theoretic framework. One aim is to use category theory to provide abstract 
characterisations of constructions like parallel composition valid throughout 
a range of different models and to provide formal means for translating be- 
tween different models. The framework helps in extending the useful concept 
of bisimulation equivalence from its familiar situation on transition systems, 
to independence models where without a careful analysis even the appropri- 
ate definition of bisimulation is not clear-cut.},
	Author = {Glynn Winskel and Mogens Nielsen},
